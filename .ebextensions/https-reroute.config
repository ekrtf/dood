files:
    /etc/nginx/conf.d/proxy.conf:
        mode: "000644"
        owner: root
        group: root
        content: |
            server {
                location / {
                    if ($http_x_forwarded_proto != 'https') {
                        return 301 https://www.doodexperience.com$request_uri;
                    }
                }
            }
